{
  "name": "egg-boilerplate-d-ts",
  "version": "1.0.0",
  "description": "egg ts boilerplate using d.ts",
  "scripts": {
    "start": "npm run tsc && egg-scripts start",
    "dev": "egg-bin dev",
    "tsc": "ets && tsc",
    "clean": "ets clean",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "tslint .",
    "ci": "npm run lint && npm run cov",
    "debug": "egg-bin debug"
  },
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "dependencies": {
    "@types/ioredis": "^4.0.6",
    "egg": "^3.2.0",
    "egg-router-plus": "^1.3.0",
    "egg-scripts": "^2.0.0",
    "egg-ts-helper": "^1.25.0",
    "moment": "^2.20.1",
    "tsconfig-paths": "^3.7.0",
    "tslib": "^1.8.1",
    "typescript": "^3.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/mockjs": "^1.0.0",
    "@types/node": "^8.5.2",
    "@types/power-assert": "^1.4.29",
    "@types/supertest": "^2.0.0",
    "@types/uuid": "^3.4.3",
    "egg-bin": "^4.3.7",
    "egg-mock": "^3.15.0",
    "mocha": "^4.1.0",
    "trash-cli": "^1.4.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0"
  },
  "author": "whxaxes<whxaxes@gmail.com>"
